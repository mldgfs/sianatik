# ملف إعدادات البيئة

# رابط API الأساسي
REACT_APP_API_BASE_URL=http://localhost:5000/api

# مفتاح API لواجهة التحكم
REACT_APP_API_KEY=your_api_key_here

# إعدادات No-IP
REACT_APP_NOIP_ENABLED=true
REACT_APP_NOIP_DOMAIN=yourdomain.no-ip.com
REACT_APP_NOIP_PORT=8080

# إعدادات التشفير
REACT_APP_ENCRYPTION_ENABLED=true
REACT_APP_ENCRYPTION_KEY=your_encryption_key_here

# إعدادات الجلسة
REACT_APP_SESSION_TIMEOUT=30
REACT_APP_MAX_CONNECTIONS=10

# إعدادات react-icons
REACT_APP_ICONS_VERSION=5.0.1
